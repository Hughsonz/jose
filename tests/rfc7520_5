#!/bin/sh -ex

prfx=tests/vectors/rfc7520_5

test "`cmd/jose dec -i $prfx.1.jwec  $prfx.1.jwk`"    == "`cat $prfx.1.pt`"
test "`cmd/jose dec -i $prfx.1.jwef  $prfx.1.jwk`"    == "`cat $prfx.1.pt`"
test "`cmd/jose dec -i $prfx.1.jweg  $prfx.1.jwk`"    == "`cat $prfx.1.pt`"
test "`cmd/jose dec -i $prfx.2.jwec  $prfx.2.jwk`"    == "`cat $prfx.2.pt`"
test "`cmd/jose dec -i $prfx.2.jwef  $prfx.2.jwk`"    == "`cat $prfx.2.pt`"
test "`cmd/jose dec -i $prfx.2.jweg  $prfx.2.jwk`"    == "`cat $prfx.2.pt`"
test "`cmd/jose dec -i $prfx.3.jwec  $prfx.3.jwk`"    == "`cat $prfx.3.pt`"
test "`cmd/jose dec -i $prfx.3.jwef  $prfx.3.jwk`"    == "`cat $prfx.3.pt`"
test "`cmd/jose dec -i $prfx.3.jweg  $prfx.3.jwk`"    == "`cat $prfx.3.pt`"
test "`cmd/jose dec -i $prfx.4.jwec  $prfx.4.jwk`"    == "`cat $prfx.4.pt`"
test "`cmd/jose dec -i $prfx.4.jwef  $prfx.4.jwk`"    == "`cat $prfx.4.pt`"
test "`cmd/jose dec -i $prfx.4.jweg  $prfx.4.jwk`"    == "`cat $prfx.4.pt`"
test "`cmd/jose dec -i $prfx.5.jwec  $prfx.5.jwk`"    == "`cat $prfx.5.pt`"
test "`cmd/jose dec -i $prfx.5.jweg  $prfx.5.jwk`"    == "`cat $prfx.5.pt`"
test "`cmd/jose dec -i $prfx.6.jwec  $prfx.6.jwk`"    == "`cat $prfx.6.pt`"
test "`cmd/jose dec -i $prfx.6.jweg  $prfx.6.jwk`"    == "`cat $prfx.6.pt`"
test "`cmd/jose dec -i $prfx.7.jwec  $prfx.7.jwk`"    == "`cat $prfx.7.pt`"
test "`cmd/jose dec -i $prfx.7.jwef  $prfx.7.jwk`"    == "`cat $prfx.7.pt`"
test "`cmd/jose dec -i $prfx.7.jweg  $prfx.7.jwk`"    == "`cat $prfx.7.pt`"
test "`cmd/jose dec -i $prfx.8.jwec  $prfx.8.jwk`"    == "`cat $prfx.8.pt`"
test "`cmd/jose dec -i $prfx.8.jwef  $prfx.8.jwk`"    == "`cat $prfx.8.pt`"
test "`cmd/jose dec -i $prfx.8.jweg  $prfx.8.jwk`"    == "`cat $prfx.8.pt`"
test "`cmd/jose dec -i $prfx.9.jwec  $prfx.9.jwk`"    == "`cat $prfx.9.pt`"
test "`cmd/jose dec -i $prfx.9.jwef  $prfx.9.jwk`"    == "`cat $prfx.9.pt`"
test "`cmd/jose dec -i $prfx.9.jweg  $prfx.9.jwk`"    == "`cat $prfx.9.pt`"
test "`cmd/jose dec -i $prfx.10.jwef $prfx.10.jwk`"   == "`cat $prfx.10.pt`"
test "`cmd/jose dec -i $prfx.10.jweg $prfx.10.jwk`"   == "`cat $prfx.10.pt`"
test "`cmd/jose dec -i $prfx.11.jwef $prfx.11.jwk`"   == "`cat $prfx.11.pt`"
test "`cmd/jose dec -i $prfx.11.jweg $prfx.11.jwk`"   == "`cat $prfx.11.pt`"
test "`cmd/jose dec -i $prfx.12.jwef $prfx.12.jwk`"   == "`cat $prfx.12.pt`"
test "`cmd/jose dec -i $prfx.12.jweg $prfx.12.jwk`"   == "`cat $prfx.12.pt`"
test "`cmd/jose dec -i $prfx.13.jweg $prfx.13.1.jwk`" == "`cat $prfx.13.pt`"
test "`cmd/jose dec -i $prfx.13.jweg $prfx.13.2.jwk`" == "`cat $prfx.13.pt`"
test "`cmd/jose dec -i $prfx.13.jweg $prfx.13.3.jwk`" == "`cat $prfx.13.pt`"
